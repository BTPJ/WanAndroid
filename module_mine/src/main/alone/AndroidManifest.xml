        <activity
            android:name=".ui.share.add.AddArticleActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".ui.share.list.MyArticleActivity"
            android:exported="false"
